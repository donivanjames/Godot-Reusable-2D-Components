enabled=PackedStringArray("res://addons/SIFB/plugin.cfg", "res://addons/SimpleFormatOnSave/plugin.cfg", "res://addons/collapse_folders/plugin.cfg", "res://addons/godot-vim/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
